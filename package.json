{
  "name": "@mu-ts/logger",
  "version": "2.2.1",
  "description": "Logging facade over Bunyan",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "matt.filion@gmail.com",
  "homepage": "https://github.com/mu-ts/logger#readme",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "build": "tsc -p .",
    "pretest": "npm run build",
    "test": "nyc ts-mocha ./**/*.spec.ts"
  },
  "files": [
    "dist/*",
    "README.md",
    "LICENSE",
    "CODE_OF_CONDUCT.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mu-ts/logger.git"
  },
  "keywords": [
    "Logger",
    "Logging",
    "AWS",
    "Lambda",
    "lamlog"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/jeremyosborne/packagejsonexample/blob/master/LICENSE.txt"
    }
  ],
  "bugs": {
    "url": "https://github.com/mu-ts/logger/issues"
  },
  "nyc": {
    "extensions": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "excludes": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "coverage/*"
    ],
    "reporter": [
      "html",
      "json",
      "text"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.18",
    "aws-sdk": "^2.498.0",
    "chai": "^4.2.0",
    "codecov": "^3.2.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "bunyan": "^1.8.12"
  }
}
